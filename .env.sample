# Database Configuration
MYSQL_ROOT_PASSWORD=root_password_123
MYSQL_DATABASE=booking_system
MYSQL_USER=booking_user
MYSQL_PASSWORD=booking_password_123

# Connection Strings
ConnectionStrings__DefaultConnection=Server=mysql;Port=3306;Database=booking_system;User=booking_user;Password=booking_password_123;

# Redis Configuration
Redis__ConnectionString=redis:6379

# JWT Configuration
Jwt__Secret=your-super-secret-jwt-key-minimum-32-characters-long-change-in-production
Jwt__Issuer=BookingSystem
Jwt__Audience=BookingSystemUsers
Jwt__ExpiryMinutes=60

# Application Settings
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://+:8080

# Hangfire
Hangfire__DashboardEnabled=true

# Feature Flags
Features__UseRedisForConcurrency=true